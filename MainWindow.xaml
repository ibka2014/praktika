        <Label Content="Введите число 1" HorizontalAlignment="Left" Margin="172,141,0,0" VerticalAlignment="Top" Height="33" Width="118"/>
        <Label Content="Введите число 2" HorizontalAlignment="Left" Margin="488,141,0,0" VerticalAlignment="Top" Height="33" Width="118" RenderTransformOrigin="0.615,0.52"/>
