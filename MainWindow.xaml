        <TextBox x:Name="TBA" HorizontalAlignment="Left" Height="33" Margin="172,184,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="104"/>
        <TextBox x:Name="TBop" HorizontalAlignment="Left" Height="33" Margin="329,184,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="104"/>
        <TextBox x:Name="TBB" HorizontalAlignment="Left" Height="33" Margin="488,184,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="104"/>
